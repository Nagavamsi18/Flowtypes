<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="flowtypes-test-suite.xml" />
	<munit:test name="flowtypes-test-suite-Main_FlowTest" doc:id="14760811-98bd-419d-ba9f-a4c6457b2420" description="Test">
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="3c00445b-042c-4586-a1c3-bd6ea93832f3" />
			<flow-ref doc:name="Flow-ref to Main_Flow" doc:id="fc1f6c5d-2ff6-4137-ae2c-af2662503e81" name="Main_Flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="71d969e6-6eaf-4ac1-a336-bf04c47e7057" expression='#[payload]' is="#[MunitTools::equalTo('This is subflow')]"/>
		</munit:validation>
	</munit:test>
	<munit:test name="flowtypes-test-suite-Main_FlowTest1" doc:id="73b62c8f-1831-47c9-83f2-676b8a4bb29d" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="d35a889f-4df2-4a6b-917d-40747b6d6f16" processor="flow-ref">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="Sub_Flow" attributeName="name" />
				</munit-tools:with-attributes>
				<munit-tools:then-call flow="Private_Flow" />
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="98b1132e-9656-4591-aa96-56ed32034b35" />
			<flow-ref doc:name="Flow-ref to Main_Flow" doc:id="5d3fb3c4-c0e8-4ecf-af7d-e120abbeeaa8" name="Main_Flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="a99980e1-d66e-4c77-88b8-37531613c07c" actual="#[payload]" expected="#['Hi this is private flow']"/>
		</munit:validation>
	</munit:test>
	<munit:test name="flowtypes-test-suite-Main_FlowTest2" doc:id="ac5d983a-7a06-4154-8877-16434222602e" description="Test">
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="7423c09c-2b5e-4686-a66c-d843eb2e24e3" />
			<flow-ref doc:name="Flow-ref to Main_Flow" doc:id="a3d60f25-eebe-4a0b-be2e-a4a290107f14" name="Main_Flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="f93ca77d-815a-4d62-bfaa-509f4d903d4f" is="#[MunitTools::notNullValue()]" expression="#[payload]"/>
		</munit:validation>
	</munit:test>


</mule>
